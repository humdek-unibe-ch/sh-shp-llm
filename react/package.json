{
  "name": "llm-chat-react",
  "version": "1.0.0",
  "description": "React-based LLM Chat Component for SelfHelp CMS",
  "main": "dist/llm-chat.umd.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build --config vite.admin.config.ts && node move-css.cjs",
    "preview": "vite preview",
    "watch": "vite build --watch && vite build --config vite.admin.config.ts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/hast": "^3.0.4",
    "bootstrap": "^4.6.0",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.8",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "terser": "^5.27.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "author": "SelfHelp Team",
  "license": "MPL-2.0",
  "keywords": [
    "react",
    "llm",
    "chat",
    "selfhelp",
    "cms"
  ]
}
